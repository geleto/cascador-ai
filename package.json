{
	"name": "cascador-ai",
	"version": "0.0.3",
	"description": "Orchestrate AI agent workflows with Nunjucks-style text templates that implicitly run async tasks in parallel",
	"author": "Angel Popov<geleto@gmail.com>",
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/geleto/cascador-ai.git"
	},
	"homepage": "https://github.com/geleto/cascador-ai#readme",
	"bugs": {
		"url": "https://github.com/geleto/cascador-ai/issues"
	},
	"keywords": [
		"ai",
		"agent",
		"vercel",
		"template",
		"async",
		"parallel",
		"concurrency",
		"llm",
		"orchestration",
		"workflow",
		"nunjucks",
		"templates"
	],
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"scripts": {
		"build": "tsc",
		"clean": "rimraf dist",
		"lint": "eslint src",
		"typecheck": "tsc --noEmit",
		"test": "mocha --node-option loader=ts-node/esm \"tests/**/*.test.ts\"",
		"test:watch": "mocha --node-option loader=ts-node/esm \"tests/**/*.test.ts\" --watch",
		"start": "tsx examples/story.ts"
	},
	"peerDependencies": {
		"ai": "^4.0.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.17.0",
		"@types/chai": "^5.0.1",
		"@types/mocha": "^10.0.10",
		"ai": "^4.3.16",
		"chai": "^5.1.2",
		"eslint": "^9.17.0",
		"mocha": "^11.0.1",
		"rimraf": "^6.0.1",
		"ts-node": "^10.9.2",
		"tsx": "^4.19.2",
		"typescript": "^5.7.2",
		"typescript-eslint": "^8.19.1"
	},
	"engines": {
		"node": ">=18.0.0",
		"npm": ">=9.0.0"
	},
	"dependencies": {
		"cascada-engine": "^0.0.9"
	}
}
